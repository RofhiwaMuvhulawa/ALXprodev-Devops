#!/bin/bash

# API endpoint for Pikachu
API_URL="https://pokeapi.co/api/v2/pokemon/pikachu"

# Make the API request and save to data.json
if curl -s "$API_URL" -o data.json; then
    echo "Successfully fetched Pikachu data"
else
    echo "Error fetching Pikachu data" > errors.txt
    exit 1
fi